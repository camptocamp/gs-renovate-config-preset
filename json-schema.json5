{
  customManagers: [
    /** Do update on the schema present in the YAML files */
    {
      customType: "regex",
      fileMatch: ["^.*\\.yaml$"],
      matchStrings: [
        ".*https://raw\\.githubusercontent\\.com/(?<depName>[^\\s]+)/(?<currentValue>[0-9\\.]+)/.*",
      ],
      datasourceTemplate: "github-tags",
    },
  ],
}
